<Application x:Class="Szofttech_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Szofttech_WPF"
             xmlns:vm="clr-namespace:Szofttech_WPF.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vm:InfoPanelGUIViewModel x:Key="infoVM"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View\Resources\DefaultStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
